{
  "name": "@enspirit/emb",
  "type": "module",
  "version": "0.0.1",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "bin": {
    "emb": "./bin/run.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc -b && cp -r src/docker/protobuf/*.proto dist/docker/protobuf/",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
