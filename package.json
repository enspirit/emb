{
  "name": "@enspirit/emb",
  "type": "module",
  "version": "0.0.1",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "bin": {
    "emb": "./bin/run.js"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:types && npm run build:ts && npm run copy:proto",
    "copy:proto": "cp -r src/docker/protobuf/*.proto dist/src/docker/protobuf/",
    "build:ts": "tsc -b",
    "build:types": "json2ts src/config/schema.json > src/config/schema.ts",
    "lint": "eslint",
    "pack": "npm run pack:macos && npm run pack:deb",
    "pack:macos": "oclif pack macos",
    "pack:deb": "oclif pack deb",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "npm run build:types && npm run test:unit",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@listr2/manager": "^3.0.1",
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-autocomplete": "^3.2.34",
    "@oclif/plugin-help": "^6.2.32",
    "@oclif/plugin-not-found": "^3.2.63",
    "@oclif/plugin-plugins": "^5.4.46",
    "@oclif/table": "^0.4.12",
    "ajv": "^8.17.1",
    "dockerode": "^4.0.7",
    "docker-compose": "^1.2.0",
    "dotenv": "^17.2.1",
    "execa": "^9.6.0",
    "find-up": "^7.0.0",
    "glob": "^11.0.3",
    "graphlib": "^2.1.8",
    "listr2": "^9.0.1",
    "luxon": "^3.7.1",
    "protobufjs": "^7.5.3",
    "simple-git": "^3.28.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fastify/deepmerge": "^3.1.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@tsconfig/node20": "^20.1.6",
    "@types/dockerode": "^3.3.42",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.2.0",
    "@types/graphlib": "^2.1.12",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint": "^9.32.0",
    "json-schema-to-typescript": "^15.0.4",
    "oclif": "^4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist/src",
    "./oclif.manifest.json"
  ],
  "main": "dist/src/index.js",
  "oclif": {
    "bin": "emb",
    "dirname": "emb",
    "commands": "./dist/src/cli/commands",
    "hooks": {
      "init": "./dist/src/cli/hooks/init"
    },
    "macos": {
      "identifier": "dev.enspirit.emb"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
